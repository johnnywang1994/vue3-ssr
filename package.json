{
  "name": "vue-ssr",
  "version": "1.0.0",
  "author": "johnnywang1994",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently 'pnpm dev:client' 'pnpm dev:server' 'pnpm start'",
    "dev:client": "cross-env NODE_ENV=development webpack --watch --config config/client.config.js",
    "dev:server": "cross-env NODE_ENV=development webpack --watch --config config/server.config.js",
    "build": "pnpm build:client && pnpm build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config config/client.config.js",
    "build:server": "cross-env NODE_ENV=production webpack --config config/server.config.js",
    "start": "nodemon --watch .ssr/entry-server.js --exec 'babel-node config/server.js'"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/runtime-corejs3": "^7.14.6",
    "@vue/compiler-sfc": "^3.1.1",
    "@vue/server-renderer": "^3.1.1",
    "babel-loader": "^8.2.2",
    "buffer": "^6.0.3",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.6.0",
    "nodemon": "^2.0.7",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "stream-browserify": "^3.0.0",
    "vue-loader": "^16.2.0",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.39.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "vue": "^3.1.1",
    "vue-meta": "3.0.0-alpha.5",
    "vue-router": "^4.0.9",
    "vuex": "^4.0.1"
  }
}
